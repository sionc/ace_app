<h1>Listing data_items</h1>

<table>
  <tr>
    <th>Data item type</th>
    <th>Statistics source</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @data_items.each do |data_item| %>
  <tr>
    <td><%= data_item.data_item_type_id %></td>
    <td><%= data_item.statistics_source_id %></td>
    <td><%= link_to 'Show', data_item %></td>
    <td><%= link_to 'Edit', edit_data_item_path(data_item) %></td>
    <td><%= link_to 'Destroy', data_item, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

